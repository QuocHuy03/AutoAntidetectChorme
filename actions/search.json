[
  {
    "action": "loop",
    "start": 1,
    "count": 4,
    "variable": "huydev",
    "do": [
      {
        "action": "open_url",
        "xpath": "",
        "value": "https://google.com"
      },
      {
        "action": "element_exists",
        "xpath": "//*[@id='APjFqb']",
        "if_true": [
          {
            "action": "input_text",
            "xpath": "//*[@id='APjFqb']",
            "value": "shop acc game"
          },
          {
            "action": "input_press_key",
            "xpath": "//*[@id='APjFqb']",
            "value": "enter"
          }
        ],
        "if_false": [{ "action": "screenshot" }],
        "stop_on_false": true
      },
      { "action": "wait", "value": "1" },
      {
        "action": "scroll",
        "value": "element",
        "xpath": "(//*[contains(@jscontroller, 'SC7lYd')]//a[@jsname='UWckNb'])[{{huydev}}]"
      },
      {
        "action": "click",
        "xpath": "(//*[contains(@jscontroller, 'SC7lYd')]//a[@jsname='UWckNb'])[{{huydev}}]"
      },
      { "action": "scroll", "value": "random" }
    ]
  }
]
