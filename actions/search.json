[
  {
    "action": "excel",
    "mode": "row",
    "path": "PROFILE_TRAFFIC.xlsx",
    "do": [
      {
        "action": "open_url",
        "value": "https://google.com"
      },
      {
        "action": "element_exists",
        "xpath": "//*[@id='APjFqb']",
        "if_true": [
          {
            "action": "input_text",
            "xpath": "//*[@id='APjFqb']",
            "value": "{{KEYWORD}}"
          },
          {
            "action": "input_press_key",
            "xpath": "//*[@id='APjFqb']",
            "value": "enter"
          }
        ],
        "if_false": [{ "action": "screenshot" }],
        "stop_on_false": true
      },
      { "action": "wait", "value": "1" },
      { "action": "scroll", "value": "random" }
    ]
  }
]
