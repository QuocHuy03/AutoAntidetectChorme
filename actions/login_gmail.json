[
  {
    "action": "excel",
    "mode": "profile",
    "path": "LOGIN_GMAIL.xlsx",
    "do": [
      {
        "action": "open_url",
        "value": "https://workspace.google.com/intl/en-US/gmail/"
      },
      {
        "action": "wait",
        "value": "1"
      },
      {
        "action": "click",
        "xpath": "//*[@id='root']/div[1]/header/div/div[5]/a[2]"
      },
      {
        "action": "element_exists",
        "xpath": "//input[@id='identifierId']",
        "if_true": [
          {
            "action": "click",
            "xpath": "//input[@id='identifierId']"
          },
          {
            "action": "input_text",
            "xpath": "//input[@id='identifierId']",
            "value": "{{MAIL}}"
          },
          {
            "action": "input_press_key",
            "xpath": "//input[@id='identifierId']",
            "value": "enter"
          }
        ],
        "if_false": [
          {
            "action": "screenshot"
          }
        ],
        "stop_on_fail": true
      },
      {
        "action": "wait",
        "value": "1"
      },
      {
        "action": "element_exists",
        "xpath": "//input[@name='Passwd']",
        "if_true": [
          {
            "action": "click",
            "xpath": "//input[@name='Passwd']"
          },
          {
            "action": "input_text",
            "xpath": "//input[@name='Passwd']",
            "value": "{{PASS}}"
          },
          {
            "action": "input_press_key",
            "xpath": "//input[@name='Passwd']",
            "value": "enter"
          }
        ],
        "if_false": [
          {
            "action": "screenshot"
          }
        ],
        "stop_on_fail": true
      },
      {
        "action": "wait",
        "value": "2"
      },
      {
        "action": "element_exists",
        "xpath": "//div[@class='T-I J-J5-Ji T-I-KE L3']",
        "if_true": [
          {
            "action": "save_to_excel",
            "path": "LOGIN_GMAIL.xlsx",
            "profile_column": "PROFILE",
            "column_save": "STATUS",
            "value": "success",
            "mode": "profile"
          }
        ],
        "if_false": [
          {
            "action": "save_to_excel",
            "path": "LOGIN_GMAIL.xlsx",
            "profile_column": "PROFILE",
            "column_save": "STATUS",
            "value": "false",
            "mode": "profile"
          }
        ]
      },
      {
        "action": "stop_script",
        "value": "Tôi thích dừng"
      }
    ]
  }
]